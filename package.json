{
  "name": "bilibili-dl",
  "version": "1.0.0",
  "description": "Bilibili Downloader",
  "scripts": {
    "lint": "turbo run lint --parallel",
    "format": "turbo run format --parallel",
    "dev": "turbo run dev",
    "build": "turbo run build",
    "pack": "node scripts/packs.js"
  },
  "keywords": [
    "bilibili.tv",
    "bilibili",
    "bstation"
  ],
  "author": "Hanif Dwy Putra S <hanifdwyputrasembiring@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "execa": "^6.1.0",
    "turbo": "^1.6.3"
  },
  "type": "module",
  "engines": {
    "node": ">=14.x"
  },
  "private": true,
  "packageManager": "yarn@3.3.0",
  "workspaces": [
    "packages/*",
    "apps/*"
  ]
}
